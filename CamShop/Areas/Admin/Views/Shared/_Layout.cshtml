    <link href="/Assets/admin/css/demo-page.css" rel="stylesheet" media="all">
    <link href="/Assets/admin/css/hover.css" rel="stylesheet" media="all">
    <!--//but-->
    <!--Delete admin-->
    <script src="/Areas/admin/assets/jquery.unobtrusive-ajax.min.js"></script>

    <script src="~/Assets/admin/js/jquery-2.1.1.min.js"></script>
